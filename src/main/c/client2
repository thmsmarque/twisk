#include "../ressource/codeC/def.h"
#define sasEntree 0
#define guichet 2
#define activite 3
#define sasSortie 1
void simulation(int ids)
{

    //SasEntree
    entrer(sasEntree);
    delai(6,3);
    //Guichet
    transfert(sasEntree,guichet);
    delai(3,1);
    //Activite restreinte
    delai(4,2)
    //SasSortie
    transfert(guichet,sasSortie);
    etapeActuelle = destination;


}

