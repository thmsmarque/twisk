#include <stdio.h>

#include "../ressource/codeC/def.h"
#define sasEntree 0
#define activite 2
#define sasSortie 1
void simulation(int ids)
{
    //SasEntree
    entrer(sasEntree);
    delai(6,3);
    //Activite
    transfert(sasEntree,activite);
    delai(3,1);
    //SasSortie
    transfert(activite,sasEntree);

}

